If $HW EQ "Real"
 Quit
Endif 

Ex Scripts:JIT_Disable

If $HW EQ "UAE"
 uae-configuration cpu_speed real cpu_multiplier 4
 uae-configuration cycle_exact true cpu_memory_exact true blitter_cycle_exact true
Endif

If $HW EQ "Mini"
 uae-configuration finegrain_cpu_speed 1024
Endif

If $HW EQ "Pi"
 uae-configuration finegrain_cpu_speed 1024
Endif
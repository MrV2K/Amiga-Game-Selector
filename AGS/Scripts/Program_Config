Echo >PIPE:a "; Stop the menu music"
Echo >PIPE:a ""
Echo >PIPE:a "Bastyplayer >NIL: x"
Echo >PIPE:a ""
Echo >PIPE:a "; Show the splash text and game info."
Echo >PIPE:a ""
Echo >PIPE:a "Ex Scripts:Splash"
Echo >PIPE:a "Ex Scripts:Disclaimer"
Echo >PIPE:a "echo *"**E[1m**E[32m Loading $PName**E[0m*""
Echo >PIPE:a "echo *"*""
Echo >PIPE:a ""
Echo >PIPE:a "; Emulator Settings"
Echo >PIPE:a ""
Echo >PIPE:a "If *$Expert EQ *"1*""
Echo >PIPE:a ""
Echo >PIPE:a " SetEnv XFave *"AGS:+  Favourites.ags/$PName.run*""
Echo >PIPE:a " SetEnv XPath *"AGS:+  My Programs.ags/$PName*""
Echo >PIPE:a " SetEnv XFile *"$PName*""
Echo >PIPE:a " SetEnv XFolder *"$PDrawer*""
Echo >PIPE:a " SetEnv XInfo *"$PFile*""
If $PType EQ "WHDLOAD"
 Echo >PIPE:a " SetEnv XType WHDLOAD"
Endif
If $PType EQ "EXE"
 Echo >PIPE:a " SetEnv XType EXE"
Endif
If $PType EQ "SCRIPT"
 Echo >PIPE:a " SetEnv XType SCRIPT"
Endif
Echo >PIPE:a " Ex Scripts:Expert_Mode_MyProgram"
Echo >PIPE:a " SKIP Clean-Up"
Echo >PIPE:a ""
Echo >PIPE:a "Else"
Echo >PIPE:a ""
Echo >PIPE:a " ; A500 Mini Settings"
Echo >PIPE:a ""
Echo >PIPE:a " If $HW EQ *"Mini*""
Echo >PIPE:a "  ; Ex Scripts:A500_Speed"
Echo >PIPE:a "  ; Ex Scripts:A1200_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_25_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_Speed"
Echo >PIPE:a "  ; Ex Scripts:NoCycle"  
Echo >PIPE:a "  ; Ex Scripts:IB"
Echo >PIPE:a "  ; Ex Scripts:FC"
Echo >PIPE:a "  ; Ex Scripts:BW"
Echo >PIPE:a "  ; Ex Scripts:NTSC"
Echo >PIPE:a " Endif"
Echo >PIPE:a ""
Echo >PIPE:a " ; Raspberry Pi Settings"
Echo >PIPE:a ""
Echo >PIPE:a " If $HW EQ *"Pi*""
Echo >PIPE:a "  ; Ex Scripts:A500_Speed"
Echo >PIPE:a "  ; Ex Scripts:A1200_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_25_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_Speed" 
Echo >PIPE:a "  ; Ex Scripts:NoCycle" 
Echo >PIPE:a "  ; Ex Scripts:IB"
Echo >PIPE:a "  ; Ex Scripts:FC"
Echo >PIPE:a "  ; Ex Scripts:BW"
Echo >PIPE:a "  ; Ex Scripts:NTSC"
Echo >PIPE:a " Endif"
Echo >PIPE:a ""
Echo >PIPE:a " ; UAE Settings"
Echo >PIPE:a ""
Echo >PIPE:a " If $HW EQ *"UAE*""
Echo >PIPE:a "  ; Ex Scripts:A500_Speed"
Echo >PIPE:a "  ; Ex Scripts:A1200_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_25_Speed"
Echo >PIPE:a "  ; Ex Scripts:A4000_Speed" 
Echo >PIPE:a "  ; Ex Scripts:NoCycle" 
Echo >PIPE:a "  ; Ex Scripts:IB"
Echo >PIPE:a "  ; Ex Scripts:BW"
Echo >PIPE:a "  ; Ex Scripts:NTSC"
Echo >PIPE:a " Endif"
Echo >PIPE:a ""
Echo >PIPE:a "Endif"
Echo >PIPE:a ""
Echo >PIPE:a "; Run The Game"
Echo >PIPE:a ""
Echo >PIPE:a "Ex Scripts:File_Log *"$PName*""
Echo >PIPE:a "cd *"$PDrawer*""
If $PType EQ "WHDLOAD"
 Echo >PIPE:a "kgiconload *"$PFile*""
Endif
If $PType EQ "EXE"
 Echo >PIPE:a "*"$PFile*""
Endif
If $PType EQ "SCRIPT"
 Echo >PIPE:a "Ex *"$PFile*""
Endif
Echo >PIPE:a ""
Echo >PIPE:a "; Pause Script"
Echo >PIPE:a ""
Echo >PIPE:a "; Ex Scripts:Pause_Mouse"
Echo >PIPE:a "; Ex Scripts:Pause_Keyboard"
Echo >PIPE:a ""
Echo >PIPE:a "; Cleanup"
Echo >PIPE:a ""
Echo >PIPE:a "LAB Clean-Up"
Echo >PIPE:a "Execute Scripts:Speed_Reset"
Echo >PIPE:a "Ex Scripts:Play_Module"
Echo >PIPE:a "AGS:"

Copy PIPE:a T:Tmp

Echo >T:PCopy "Copy T:Tmp TO *"AGS:+  My Programs.ags/$PName.run*""
Ex T:PCopy

Delete >NIL: T:Tmp
Delete >NIL: T:PCopy
